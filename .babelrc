{
    "presets": [
        "es2015",
        "react"
    ],
    "plugins": [
        "lodash",
        "syntax-decorators",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-react-jsx"
    ],
    "env": {
        "production": {
            "plugins": [
                ["transform-react-remove-prop-types", { "removeImport": true }],
                "transform-react-constant-elements",
                "transform-react-inline-elements"
            ]
        }
    }
}
